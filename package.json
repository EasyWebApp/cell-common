{
  "name": "boot-cell",
  "version": "0.2.0",
  "description": "Web components library based on WebCell engine & BootStrap v4",
  "main": "index.js",
  "engines": {
    "node": "^6.5.0"
  },
  "scripts": {
    "start": "web-server -o test/",
    "lint": "eslint source/ test/",
    "check": "mocha -r babel-register -r babel-polyfill -r should -c --inline-diffs",
    "test": "npm run check -- --exit",
    "debug": "npm run check -- --inspect-brk",
    "precommit": "npm run lint  &&  npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyWebApp/BootCell.git"
  },
  "keywords": [
    "component",
