    "web-cell"
  ],
  "author": "shiy2008@gmail.com",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/EasyWebApp/BootCell/issues"
  },
  "homepage": "https://github.com/EasyWebApp/BootCell#readme",
  "devDependencies": {
    "babel-env": "^2.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "koapache": "^0.2.3",
    "mocha": "^5.2.0",
    "puppeteer": "^1.4.0",
    "should": "^13.2.1"
  },
  "dependencies": {
    "bootstrap": "^4.1.1"
  },
  "babel": {
    "presets": [
